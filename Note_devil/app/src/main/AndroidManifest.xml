<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.note.note_devil" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".note_list"
            android:label="@string/title_activity_note_list" >
        </activity>
        <activity
            android:name=".note_edit"
            android:label="@string/title_activity_note_edit" >
        </activity>
        <activity
            android:name=".note_todo"
            android:label="@string/title_activity_note_todo" >
        </activity>
        <activity
            android:name=".note_finished"
            android:label="@string/title_activity_note_finished">
        </activity>
    </application>

</manifest>
